<FindBugsFilter>
    <!--Classes geradas pelo Android ignoradas-->
    <Match>
        <Class name="~.*R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*Manifest\$.*"/>
    </Match>
    <!--Classes do Android Annotations ignoradas-->
    <Match>
        <Class name="~.*_"/>
    </Match>
    <!--Classes de testes ignoradas-->
    <Match>
        <Class name="~.*\.*Test"/>
    </Match>
    <!--Bug ignorado, pois entra em conflito com Android Annotations-->
    <Match>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    <Match>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>
    <!--Classe de constantes ignorada-->
    <Match>
        <Class name="~.*Constants.*"/>
    </Match>
</FindBugsFilter>